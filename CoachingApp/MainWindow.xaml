                    <Image VerticalAlignment="Top" Height="252.5" Source="generic-user-purple.png"/>
                    <Button x:Name="menuItemButton" Content="" Background="{x:Null}" BorderBrush="{x:Null}" Foreground="{x:Null}" Grid.Row="1"/>
                    <Grid/>
                </Grid>
                <Grid x:Name="menuItemGrid2" Grid.Column="1" Grid.Row="0">
