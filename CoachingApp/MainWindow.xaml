                </Grid>
                <Grid x:Name="menuItemGrid4" Grid.Column="3" Grid.Row="0">
                    <TextBlock x:Name="menuItem4Tile"  TextWrapping="Wrap" Text="My Team" VerticalAlignment="Bottom" TextAlignment="Center" Height="56.5" FontSize="36" FontWeight="Bold"/>
                    <Image VerticalAlignment="Top" Height="252.5" Source="generic-user-purple.png"/>
                    <Button x:Name="menuItemButton4" Content="" Background="{x:Null}" BorderBrush="{x:Null}" Foreground="{x:Null}" Grid.Row="1"/>
                </Grid>
                <Grid x:Name="menuItemGrid5" Grid.Column="0" Grid.Row="1">
                    <TextBlock x:Name="menuItem5Title" TextWrapping="Wrap" Text="Planner" VerticalAlignment="Bottom" TextAlignment="Center" Height="56.5" FontSize="36" FontWeight="Bold"/>
                    <Image VerticalAlignment="Top" Height="252.5" Source="generic-user-purple.png"/>
                    <Button x:Name="menuItemButton5" Content="" Background="{x:Null}" BorderBrush="{x:Null}" Foreground="{x:Null}" Grid.Row="1"/>
                    <Grid/>
                </Grid>
                <Grid x:Name="menuItemGrid6" Grid.Column="1" Grid.Row="1">
                    <TextBlock x:Name="menuItem6Title" TextWrapping="Wrap" Text="Resouces" VerticalAlignment="Bottom" TextAlignment="Center" Height="56.5" FontSize="36" FontWeight="Bold"/>
                    <Image VerticalAlignment="Top" Height="252.5" Source="generic-user-purple.png"/>
                    <Button x:Name="menuItemButton6" Content="" Background="{x:Null}" BorderBrush="{x:Null}" Foreground="{x:Null}" Grid.Row="1"/>
                    <Grid/>
                </Grid>
                <Grid x:Name="menuItemGrid7" Grid.Column="2" Grid.Row="1">
                    <TextBlock x:Name="menuItem7Tile"  TextWrapping="Wrap" Text="NewsFeed" VerticalAlignment="Bottom" TextAlignment="Center" Height="56.5" FontSize="36" FontWeight="Bold"/>
                    <Image VerticalAlignment="Top" Height="252.5" Source="generic-user-purple.png"/>
                    <Button x:Name="menuItemButton7" Content="" Background="{x:Null}" BorderBrush="{x:Null}" Foreground="{x:Null}" Grid.Row="1"/>
                </Grid>
                <Grid x:Name="menuItemGrid8" Grid.Column="3" Grid.Row="1">
                    <TextBlock x:Name="menuItem8Tile"  TextWrapping="Wrap" Text="Team Stats" VerticalAlignment="Bottom" TextAlignment="Center" Height="56.5" FontSize="36" FontWeight="Bold"/>
                    <Image VerticalAlignment="Top" Height="252.5" Source="generic-user-purple.png"/>
                    <Button x:Name="menuItemButton8" Content="" Background="{x:Null}" BorderBrush="{x:Null}" Foreground="{x:Null}" Grid.Row="1"/>
                </Grid>
            </Grid>
        </Grid>
    </Grid>
</Window>
